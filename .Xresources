! Urxvt Settings
URxvt.scrollBar: false
URxvt.scrollBar_right: false
! URxvt*depth: 32
URxvt*termName: rxvt
URxvt*skipBuiltinGlyphs: true
URxvt.background: [70]#000000
! URxvt*internalBorder: 50
URxvt.saveLines: 0
URxvt.keysym.Control-Mod-v: eval:paste_escaped
! paste_clipboard ; paste_escaped ; paste
urxvt.keysym.Control-Mod-c: eval:selection_to_clipboard
URxvt.perl-ext-common: default
